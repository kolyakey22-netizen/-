    private func getRecommendations() -> String {
        var recommendations: [String] = []
        let stats = getQueueStatus()
        let speedStats = getStats()

        if stats.pending > 100 && activeThreads < maxConcurrentThreads {
            recommendations.append("‚Ä¢ –ú–æ–∂–Ω–æ —É–≤–µ–ª–∏—á–∏—Ç—å –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø–æ—Ç–æ–∫–æ–≤")
        }

        if speedStats.failed > speedStats.successful * 0.3 {
            recommendations.append("‚Ä¢ –ú–Ω–æ–≥–æ –æ—à–∏–±–æ–∫ - –ø—Ä–æ–≤–µ—Ä—å—Ç–µ –∞–∫–∫–∞—É–Ω—Ç—ã")
        }

        if speedStats.averageProcessingTime > 10.0 {
            recommendations.append("‚Ä¢ –í—ã—Å–æ–∫–æ–µ –≤—Ä–µ–º—è –æ—Ç–≤–µ—Ç–∞ - –≤–æ–∑–º–æ–∂–Ω–æ –ø—Ä–æ–±–ª–µ–º—ã —Å —Å–µ—Ç—å—é")
        }

        return recommendations.isEmpty ? "‚Ä¢ –í—Å–µ –ø–∞—Ä–∞–º–µ—Ç—Ä—ã –æ–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω—ã" : recommendations.joined(separator: "\n")
    }
}
