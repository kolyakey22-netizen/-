       }
    }

    func cleanup() {
        cleanupTask?.cancel()
        cleanupTask = nil
        stopPersistentProcess()
    }

    deinit {
        // –ü—Ä–æ—Å—Ç–æ –æ—Ç–º–µ–Ω—è–µ–º –∑–∞–¥–∞—á—É –æ—á–∏—Å—Ç–∫–∏, –æ–Ω–∞ —Å–∞–º–∞ –≤—ã–∑–æ–≤–µ—Ç stopPersistentProcess
        cleanupTask?.cancel()
    }
}
