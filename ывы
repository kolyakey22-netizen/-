    func getQueueStats() async -> (total: Int, active: Int, pending: Int, completed: Int) {
        let total = queue.count
        let active = queue.filter { $0.status == .processing }.count
        let pending = queue.filter { $0.status == .pending }.count
        let completed = queue.filter { $0.status == .completed }.count

        return (total, active, pending, completed)
    }

    func clearCompleted() async {
        queue.removeAll { $0.status == .completed || $0.status == .failed }
    }
}
